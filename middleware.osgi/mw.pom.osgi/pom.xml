<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.universAAL.middleware</groupId>
    <artifactId>mw.pom</artifactId>
    <version>1.3.1-SNAPSHOT</version>
    <relativePath>../../pom</relativePath>
  </parent>
  <groupId>org.universAAL.middleware</groupId>
  <artifactId>mw.pom.osgi</artifactId>
  <version>1.3.1-SNAPSHOT</version>
  <packaging>pom</packaging>
  <name>universAAL Middleware Suite (OSGi)</name>
  <description>The set of artefacts realizing the universAAL middleware based on the PERSONA codebase. (OSGi)</description>
  <modules>
    <module>../mw.container.xfaces.osgi</module>
    <module>../mw.container.osgi</module>
    <module>../mw.acl.interfaces.osgi</module>
    <module>../mw.acl.rosgi</module>
    <module>../mw.acl.upnp.osgi</module>
    <module>../mw.acl.bridge.osgi</module>
    <module>../mw.bus.model.osgi</module>
    <module>../mw.data.representation.osgi</module>
    <module>../mw.data.serialization.osgi</module>
    <module>../mw.bus.service.osgi</module>
    <module>../mw.bus.service.aapi.osgi</module>
    <module>../mw.bus.context.osgi</module>
    <module>../mw.bus.ui.osgi</module>
    <module>../mw.bus.tracker.osgi</module>
    <module>composite</module>
  </modules>
  <scm>
    <connection>${svn.base}/middleware.osgi/${project.artifactId}</connection>
    <developerConnection>${svn.base}/middleware.osgi/${project.artifactId}</developerConnection>
    <url>${svn.base.url}middleware.osgi/${project.artifactId}</url>
  </scm>
  <properties>
    <separatedGroupIds>org.universAAL.middleware</separatedGroupIds>
    <groupIdMatchString>^org\.universAAL\.middleware</groupIdMatchString>
    <artifactIdMatchString>^mw\..*\.osgi$</artifactIdMatchString>
    <nameMatchString>^universAAL Middleware .* \(OSGi\)$</nameMatchString>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.pom.core</artifactId>
        <version>1.3.1-SNAPSHOT</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>org.apache.felix</groupId>
        <artifactId>org.osgi.core</artifactId>
        <version>1.0.1</version>
        <scope>provided</scope>
      </dependency>
      <dependency>
        <groupId>org.apache.felix</groupId>
        <artifactId>org.osgi.compendium</artifactId>
        <version>1.4.0</version>
        <scope>provided</scope>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.container.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.container.xfaces.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.acl.interfaces.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.acl.rosgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.acl.upnp.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.acl.bridge.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.bus.context.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.bus.ui.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.bus.model.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.bus.service.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.bus.service.aapi.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.data.representation.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.data.serialization.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.bus.tracker.osgi</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.universAAL.support</groupId>
        <artifactId>itests</artifactId>
        <version>1.3.0</version>
        <scope>test</scope>
      </dependency>
      <dependency>
        <groupId>org.universAAL.middleware</groupId>
        <artifactId>mw.composite</artifactId>
        <version>1.3.1-SNAPSHOT</version>
      </dependency>
    </dependencies>
  </dependencyManagement>
  <build>
    <plugins>
      <plugin>
        <groupId>org.universAAL.support</groupId>
        <artifactId>uaal-maven-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>build-helper-maven-plugin</artifactId>
      </plugin>
    </plugins>
  </build>
</project>
